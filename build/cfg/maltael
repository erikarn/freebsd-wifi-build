# This is a big-endian mips build

BUILDNAME=mipsel
TARGET=mips
TARGET_ARCH=mipsel
TARGET_CPUTYPE=mips32
KERNCONF=MALTA
LOCAL_DIRS=" "

X_MAKEFS_ENDIAN=le
X_ROOTFS_DEV="/dev/ada0"
X_FULL_FSSIZE="4294967296"
X_KERNSUFFIX=MALTAEL

# Building the firmware image
X_BUILD_BUILD_IMG_DEFAULTS="fullroot fullfsimage"
